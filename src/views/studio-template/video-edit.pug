//-
    pug param
    -sessionUser
    -foundVideoToEdit
extends ../partials/html-head.pug
block page-css
    link(rel="stylesheet" href="/assets/css/bundled.videoEdit.css" type="text/css")
block page-js
    script(src="/assets/js/bundled.videoEdit.js" type="text/javascript" defer)
block wtcp-app
    wtcp-app
        wtcp-dialog-manager.wtcp-app
        wtcp-nitrate.wtcp-app
        wtcp-toast-manager(alignment="center").wtcp-app
        wtsp-sponsors-only-video-post-manager.wtcp-app
        wtcp-permanet-operational-error-manager.wtcp-app
        wtcp-a11y-announcer.wtcp-app
        wtcp-entity-page(modern first-load)#entity-page.loaded.wtcp-app
            div#main-container.wtcp-entity-page
                tp-wt-iron-overlay-backdrop(tabindex="-1")#nav-backdrop.wtcp-entity-page
                wtcp-top-banner(color-theme)#top-banner.wtcp-entity-page
                wtcp-header.wtcp-entity-page
                    header(is-search-visible).wtcp-header
                        wtcp-icon-button(
                            icon="menu" track-click tabindex="0" modern
                            aria-disabled="false" aria-label="메뉴 접기" role="button"
                        )#collapse-expand-icon.rtl-flip.wtcp-header
                            tp-wt-iron-icon.remove-defaults.wtcp-icon-button
                                svg(
                                    viewBox="0 0 24 24" preserveAspectRatio="xMidYMid meet"
                                    focusable="false" aria-hidden="true"
                                    style="pointer-events: none; display: block; width: 100%; height: 100%;"
                                ).tp-wt-iron-icon
                                    g.tp-wt-iron-icon
                                        path(d="M21,6H3V5h18V6z M21,11H3v1h18V11z M21,17H3v1h18V17z").tp-wt-iron-icon
                        wtcp-home-button(track-click).wtcp-header
                            a(
                                target="_self" href=`/channel/${sessionUser.user_channel.channel_id}`
                            )#home-button.wtcp-home-button
                                img(
                                    height="24" alt="WeTube 스튜디오 대시보드"
                                    src="https://www.gstatic.com/youtube/img/creator/yt_studio_logo_v2_darkmode.svg"
                                ).wtcp-home-button
                        wtcp-button.wtcp-header
                        wtcp-omnisearch(modern)#omnisearch.wtcp-header
                            tp-wt-iron-overlay-backdrop#backdrop.wtcp-omnisearch
                            div(tabindex="-1")#search-layer.wtcp-omnisearch
                                tp-wt-iron-icon.search-icon.wtcp-omnisearch
                                    svg(
                                        viewBox="0 0 24 24" preserveAspectRatio="xMidYMid meet"
                                        focusable="false" aria-hidden="true"
                                        style="pointer-events: none; display: block; width: 100%; height: 100%;"
                                    ).tp-wt-iron-icon
                                        g.tp-wt-iron-icon
                                            path(
                                                d="M20.87,20.17l-5.59-5.59C16.35,13.35,17,11.75,17,10c0-3.87-3.13-7-7-7s-7,3.13-7,7s3.13,7,7,7c1.75,0,3.35-0.65,4.58-1.71 l5.59,5.59L20.87,20.17z M10,16c-3.31,0-6-2.69-6-6s2.69-6,6-6s6,2.69,6,6S13.31,16,10,16z"
                                            ).tp-wt-iron-icon
                                wtcp-ve(track-click role="none").wtcp-omnisearch
                                    form(name="searchForm").wtcp-omnisearch
                                        input(
                                            autocomplete="off"
                                            aria-controls="results"
                                            placeholder="채널에서 검색하기"
                                            aria-activedescendant="video-result--1"
                                            aria-expanded="true"
                                        )#query-input.wtcp-omnisearch
                        div.right-section.wtcp-header
                            div#right-section-content.right-section-content.wtcp-header
                                div.icons-section.wtcp-header
                                    wtcp-help#help-widget.wtcp-header
                                        wtcp-icon-button(
                                            icon="help-outline" test-id="header-help-icon"
                                            tooltip-lebel="도움말" track-click="true"
                                            tabindex="0" modern aria-disabled="false"
                                            aria-label="도움말" role="button"
                                        )#help-icon.wtcp-header
                                            tp-wt-iron-icon.remove-defaults.wtcp-icon-button
                                                svg(
                                                    viewBox="0 0 24 24" preserveAspectRatio="xMidYMid meet"
                                                    focusable="false" aria-hidden="true"
                                                    style="pointer-events: none; display: block; width: 100%; height: 100%;"
                                                ).tp-wt-iron-icon
                                                    g.tp-wt-iron-icon
                                                        path(d="M15.36,9.96c0,1.09-0.67,1.67-1.31,2.24c-0.53,0.47-1.03,0.9-1.16,1.6L12.85,14h-1.75l0.03-0.28 c0.14-1.17,0.8-1.76,1.47-2.27c0.52-0.4,1.01-0.77,1.01-1.49c0-0.51-0.23-0.97-0.63-1.29c-0.4-0.31-0.92-0.42-1.42-0.29 c-0.59,0.15-1.05,0.67-1.19,1.34L10.32,10H8.57l0.06-0.42c0.2-1.4,1.15-2.53,2.42-2.87c1.05-0.29,2.14-0.08,2.98,0.57 C14.88,7.92,15.36,8.9,15.36,9.96z M12,18c0.55,0,1-0.45,1-1s-0.45-1-1-1s-1,0.45-1,1S11.45,18,12,18z M12,3c-4.96,0-9,4.04-9,9 s4.04,9,9,9s9-4.04,9-9S16.96,3,12,3 M12,2c5.52,0,10,4.48,10,10s-4.48,10-10,10S2,17.52,2,12S6.48,2,12,2L12,2z")
                                wtcp-button(
                                    label="만들기" tooltip-label="동영상 또는 게시물 만들기"
                                    track-click type="outline" tabindex="-1"
                                    button-style="default" icon-alignment="start"
                                    modern icon="wt-sys-icons:video_call" aria-disabled="false"
                                    role="button"
                                )#create-icon.wtcp-header
                                    wtcp-button-shape.wtcp-button
                                        button(
                                            aria-label="만들기"
                                        ).wtcp-button-shape-impl.wtcp-button-shape-impl--outline.wtcp-button-shape-impl--mono.wtcp-button-shape-impl--size-m.wtcp-button-shape-impl--icon-leading
                                            div(aria-hidden="true").wtcp-button-shape-impl__icon
                                                wt-icon(style="width: 24px; height: 24px;")
                                                    svg(
                                                        viewBox="0 0 24 24" preserveAspectRatio="xMidYMid meet"
                                                        focusable="false" aria-hidden="true"
                                                        style="pointer-events: none; display: block; width: 100%; height: 100%;"
                                                    ).wt-icon
                                                        g.wt-icon
                                                            path(
                                                                d="M14,13h-3v3H9v-3H6v-2h3V8h2v3h3V13z M17,6H3v12h14v-6.39l4,1.83V8.56l-4,1.83V6 M18,5v3.83L22,7v8l-4-1.83V19H2V5H18L18,5 z"
                                                            ).wt-icon
                                            div.wtcp-button-shape-impl__button-text-content
                                                |만들기
                                wtd-topbar-menu-button-renderer#account-button.avatar.wtcp-header
                                    button(
                                        aria-haspopup="true" aria-label="Account"
                                    )#avatar-btn.wtd-topbar-menu-button-renderer
                                        wt-img-shadow(
                                            height="24" width="24"
                                            style="background-color: transparent;"
                                            loaded
                                        ).wtd-topbar-menu-button-renderer.no-transition
                                            img(
                                                draggable="false" alt height="32" width="32"
                                                src=sessionUser.avatar
                                            )#img.wt-img-shadow
                wtcp-banner.entity-banner.wtcp-entity-page.closing